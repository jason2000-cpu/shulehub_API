{
  "functions": {
    "api/server.js": {
      "memory": 1024,
      "includeFiles": "db.json"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://shulehub-api-5yj9.vercel.app/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/api/server.js"
    }
  ]
}
